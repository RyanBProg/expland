{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "nodemon server.ts",
    "dev-no-ts": "nodemon --exec ts-node --transpile-only server.ts",
    "prisma:generate": "npx prisma generate --schema=database/prisma/schema.prisma",
    "prisma:migrate:dev": "npx prisma migrate dev --schema=database/prisma/schema.prisma",
    "prisma:migrate:deploy": "npx prisma migrate deploy --schema=database/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=database/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.18",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.26.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
